{
  "name": "angular-app",
  "version": "0.0.0",
  "config": {
    "region": "eu-north-1",
    "appBucket": "buzzbrain-staging---app",
    "distributionId": "ERQLFIYH6MMYP"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "deleteobjectsins3": "aws s3 rm s3://$npm_package_config_appBucket --recursive",
    "copytos3": "aws s3 cp dist/angular-app/browser s3://$npm_package_config_appBucket --recursive",
    "invalidate-cf-cache": "aws cloudfront create-invalidation --distribution-id $npm_package_config_distributionId --paths /index.html",
    "deploy": "npm run build && npm run deleteobjectsins3 && npm run copytos3 && npm run invalidate-cf-cache"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^19.2.6",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.6",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@ngrx/effects": "^19.0.1",
    "@ngrx/store": "^19.0.1",
    "@ngrx/store-devtools": "^19.0.1",
    "jwt-decode": "^4.0.0",
    "ngrx-store-localstorage": "^19.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.4",
    "@angular/cli": "^19.2.4",
    "@angular/compiler-cli": "^19.2.0",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.7",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ngx-tailwind": "^4.0.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.6",
    "typescript": "~5.7.2"
  }
}
